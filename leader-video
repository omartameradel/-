<script>
(function(){
    'use strict';
    const t=document.getElementById('mobile-menu-toggle'),
          m=document.getElementById('primary-navigation'),
          o=document.getElementById('menu-overlay'),
          b=document.body,
          h=document.querySelector('.site-header');
    
    function open(){m.classList.add('is-open');o.classList.add('active');b.classList.add('menu-open');t.classList.add('active');t.setAttribute('aria-expanded','true')}
    function close(){m.classList.remove('is-open');o.classList.remove('active');b.classList.remove('menu-open');t.classList.remove('active');t.setAttribute('aria-expanded','false')}
    
    if(t)t.addEventListener('click',()=>m.classList.contains('is-open')?close():open());
    if(o)o.addEventListener('click',close);
    document.addEventListener('keydown',e=>{if(e.key==='Escape'&&m.classList.contains('is-open'))close()});
    m.querySelectorAll('a').forEach(l=>l.addEventListener('click',()=>{if(window.innerWidth<=768)close()}));
    
    let s=0;
    window.addEventListener('scroll',()=>{const c=window.pageYOffset;h.classList.toggle('scrolled',c>50);s=c},{passive:true});
})();
</script>
