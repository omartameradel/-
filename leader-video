// PWA Install Prompt
let deferredPrompt;
window.addEventListener('beforeinstallprompt', (e) => {
  e.preventDefault();
  deferredPrompt = e;
  
  const installBtn = document.createElement('button');
  installBtn.innerHTML = 'ğŸ“² Ø£Ø¶Ù Ù„Ù„Ø´Ø§Ø´Ø©';
  installBtn.style.cssText = 'position:fixed;bottom:90px;left:30px;padding:12px 20px;background:#667eea;color:#fff;border:none;border-radius:50px;cursor:pointer;z-index:98;font-weight:600;box-shadow:0 4px 15px rgba(102,126,234,0.4)';
  
  installBtn.onclick = async () => {
    if (deferredPrompt) {
      deferredPrompt.prompt();
      await deferredPrompt.userChoice;
      deferredPrompt = null;
      installBtn.remove();
    }
  };
  
  document.body.appendChild(installBtn);
});
</script>

