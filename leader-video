        html += `
          <a 
            href="${proxyUrl}" 
            class="download-link-new ${isWatermark ? 'watermark' : ''}"
            download
            target="_blank"
            rel="noopener noreferrer"
          >
            ${link.quality || 'Download'}
          </a>
        `;
      });
    }
    
    html += `
          </div>
        </div>
      </div>
    `;
    
    resultContainer.innerHTML = html;
    
    // Smooth scroll Ù„Ù„Ù†ØªÙŠØ¬Ø©
    resultContainer.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
  }
  
  // ============================================
  // 6. Ø§Ù„Ø£Ø³Ø¦Ù„Ø© Ø§Ù„Ø´Ø§Ø¦Ø¹Ø© (FAQ)
  // ============================================
  document.querySelectorAll('.faq-question').forEach(button => {
    button.addEventListener('click', function() {
      const expanded = this.getAttribute('aria-expanded') === 'true';
      const answer = this.nextElementSibling;
      
      // ØªØ¨Ø¯ÙŠÙ„ Ø§Ù„Ø­Ø§Ù„Ø©
      this.setAttribute('aria-expanded', !expanded);
      answer.hidden = expanded;
      
      // Ø¥Ø¶Ø§ÙØ©/Ø¥Ø²Ø§Ù„Ø© class Ù„Ù„Ø£Ù†ÙŠÙ…ÙŠØ´Ù†
      if (!expanded) {
        answer.style.maxHeight = answer.scrollHeight + 'px';
      } else {
        answer.style.maxHeight = '0';
      }
    });
  });
  
  // ============================================
  // 7. Ø§Ù„Ù‡ÙŠØ¯Ø± Ø§Ù„Ù…Ø«Ø¨Øª (Sticky Header)
  // ============================================
  const header = document.querySelector('.site-header');
  if (header) {
    window.addEventListener('scroll', function() {
      const currentScroll = window.pageYOffset;
      
      if (currentScroll > 100) {
        header.classList.add('scrolled');
      } else {
        header.classList.remove('scrolled');
      }
    });
  }
  
})();
</script>


</main><!-- #primary -->

